{
  "manifest_version": 2,

  "name": "Star History",
  "description": "A chrome extension showing star history graph of current github repository",
  "version": "1.0",

  "browser_action": {
    "default_icon": "./assets/ico.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",                 
    "https://api.github.com/"
  ]
}
